<div class="page-header">
  <h3>Doctor #<%= @doctor.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Firstname</dt>
      <dd><%= @doctor.firstname %></dd>

      <dt>Lastname</dt>
      <dd><%= @doctor.lastname %></dd>

      <dt>Email</dt>
      <dd><%= @doctor.email %></dd>

      <dt>Password</dt>
      <dd><%= @doctor.password %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/doctors" class="btn btn-primary">
        Back
      </a>
    </div>
  </div>
</div>
